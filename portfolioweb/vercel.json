{
  "builds": [
    {
      "src": "gunicorn_start.sh",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "fredmagaweb/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "gunicorn_start.sh"
    }
  ]
}
